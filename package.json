{
  "name": "angularjs-store",
  "version": "2.0.4",
  "description": "A tools to easily manage your state in AngularJS Application",
  "main": "lib/angularjs-store.js",
  "scripts": {
    "build": "webpack --config webpack.lib.js",
    "build:demo": "webpack --config webpack.demo.prod.js",
    "build:docs": "gitbook build ./docs",
    "serve:demo": "webpack-dev-server --config webpack.demo.dev.js",
    "serve:docs": "gitbook serve ./docs",
    "test": "mocha-webpack --webpack-config webpack.test.js \"specs/*.spec.js\"",
    "test:watch": "mocha-webpack --webpack-config webpack.test-watch.js --watch \"specs/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rannie-peralta/angularjs-store.git"
  },
  "keywords": [
    "angularjs",
    "angular1",
    "store",
    "state",
    "predictable",
    "manager",
    "management",
    "observable",
    "emitter",
    "flux",
    "redux",
    "reactive"
  ],
  "author": "Rannie Peralta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rannie-peralta/angularjs-store/issues"
  },
  "homepage": "https://github.com/rannie-peralta/angularjs-store#readme",
  "dependencies": {
    "angular": "^1.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "benv": "^3.3.0",
    "bootstrap": "^4.1.3",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "gitbook-cli": "^2.3.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "10.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-webpack": "next",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  }
}
